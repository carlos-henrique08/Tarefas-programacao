algoritmo "Positivo Negativo"
 // Faça uma algoritmo que ao ser informado
  //um número verifique se é positivo ou negativo.
var

  Num:real
  
inicio
  escreva(" Informe um Número: ")
  leia(Num)
  
  escreval(" Número: ", Num)
  
  se Num > 0 entao
      escreva("Positivo")
      senao
      escreva("Negativo")
  
  
  fimse
fimalgoritmo
