algoritmo "Exe10-"
// fazer um algoritmo para calcular área do triângulo

var
 Base, Altura, Area:real
 
inicio
  escreva("Informe a base do  triangulo: ")
  leia(Base)
  escreva("Informe a altura triangulo: ")
  leia(Altura)
  
  Area <- (Base * Altura) / 2

  escreva("**Área do triângulo é: ", Area)
fimalgoritmo
