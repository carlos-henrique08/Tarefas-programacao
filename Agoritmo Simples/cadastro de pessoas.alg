algoritmo "Exe8-"
// fazer um algoritmo para cadastro de pessoas

var
 Nome, Genero:caracter
 Idade:inteiro
 
inicio
  escreva("Infome o seu nome: ")
  leia(Nome)
  escreva("Infome o sua idade: ")
  leia(Idade)
  escreva("Infome o seu genero: ")
  leia(Genero)

  escreval("nome: ",Nome)
  escreval("idade: ",Idade)
  escreval("genero: ",Genero)
fimalgoritmo
