algoritmo "Area do Quadrado"
// Area so quadrado

var
  Base, Altura, Area:real
  
inicio
  escreva(" Informe a base do Quadrado: ")
  leia(Base)
  escreva(" Informe a Altura do Quadrado: ")
  leia(Altura)
  
  Area <- Base * Altura

  escreval("--O Resultado da Área do Quadrado-- ")
  escreval(" Área do Quadrado: ", Area)
fimalgoritmo
