algoritmo "Exe 1 "

// 4 - fazer um algoritmo de número sucessor e antecessor

var
  Num:inteiro
  Antecessor:inteiro
  Sucessor:inteiro
  
inicio
   escreva("Digite um Numero: ")
   leia(Num)
   Antecessor <- Num - 1
   sucessor <- Num + 1
   escreval("antecessor: " ,Antecessor)
   escreval("sucessor: " ,Sucessor)

fimalgoritmo
