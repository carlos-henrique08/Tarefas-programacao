algoritmo "Exe9-"
// fazer um algoritmo para cadastrar clientes
var
  NC, Produto:caracter
  IDC:inteiro
  
inicio
escreva("Digite o nome do cliente:")
leia(NC)
escreva("Digite a idade do cliente:")
leia(IDC)

escreva("Digite o nome do produto:")
leia(Produto)

escreval("cliente: ",NC, "\idade: ", IDC)

fimalgoritmo
